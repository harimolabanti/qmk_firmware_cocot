// Useful shortcuts
C3A(ALT_X, C, GUI_D, PS(SS_LGUI("s")))
C2A(A, R, PS(SS_LGUI("z")))
C2A(W, F, PS(SS_LGUI("c")))
C2A(F, P, PS(SS_LGUI("v")))
C2A(W, P, PS(SS_LGUI("x")))
C3A(S, T, G, PS(SS_DOWN(X_LGUI)SS_TAP(X_SLSH)SS_UP(X_LGUI)SS_TAP(X_T)))
C3A(F, P, B, PS(SS_DOWN(X_LSFT)SS_TAP(X_F10)SS_UP(X_LSFT)))

// Mouse shortcuts
C2(J, L, BTN4)
C2(K, H, BTN5)

// Navigation
C2(U, E, UP)
C2(L, N, LEFT)
C2(Y, I, RIGHT)
C2(E, COMM, DOWN)
C2(J, M, PGUP)
C2(M, K, PGDN)
C2(N, M, HOME)
C2(I, O, END)

// Move up and down in 5 increments.
C2A(N, U, PS(SS_TAP(X_UP)SS_TAP(X_UP)SS_TAP(X_UP)SS_TAP(X_UP)SS_TAP(X_UP)))
C2A(U, I, PS(SS_TAP(X_DOWN)SS_TAP(X_DOWN)SS_TAP(X_DOWN)SS_TAP(X_DOWN)SS_TAP(X_DOWN)))
// New line above and below.
C2A(L, M, PS(SS_TAP(X_HOME)SS_TAP(X_ENT)SS_TAP(X_UP)))
C2A(N, K, PS(SS_TAP(X_END)SS_TAP(X_ENT)))

// Select current line and move up<.
C3A(L, U, Y, PS(SS_TAP(X_END)SS_DOWN(X_LSFT)SS_TAP(X_UP)SS_TAP(X_END)SS_UP(X_LSFT)))
// Duplicate current line
C3A(W, F, P, PS(SS_TAP(X_END)SS_DOWN(X_LSFT)SS_TAP(X_UP)SS_TAP(X_END)SS_UP(X_LSFT)SS_LGUI("c")SS_LGUI("v")SS_LGUI("v")))
// Delete current line
C3A(H, COMM, DOT, PS(SS_TAP(X_END)SS_DOWN(X_LSFT)SS_TAP(X_HOME)SS_UP(X_LSFT)SS_TAP(X_DEL)))

C2A(N, E, PNPMS(SS_DOWN(X_LALT)SS_DOWN(X_LEFT), SS_UP(X_LALT)SS_UP(X_LEFT), SS_DOWN(X_LCTL)SS_DOWN(X_LEFT), SS_UP(X_LCTL)SS_UP(X_LEFT)))
C2A(E, I, PNPMS(SS_DOWN(X_LALT)SS_DOWN(X_RIGHT), SS_UP(X_LALT)SS_UP(X_RIGHT), SS_DOWN(X_LCTL)SS_DOWN(X_RIGHT), SS_UP(X_LCTL)SS_UP(X_RIGHT)))
C3A(R, S, T, PNPMS(SS_DOWN(X_LALT)SS_DOWN(X_DEL), SS_UP(X_LALT)SS_UP(X_DEL), SS_DOWN(X_LCTL)SS_DOWN(X_DEL), SS_UP(X_LCTL)SS_UP(X_DEL)))
C3A(N, E, I, PNPMS(SS_DOWN(X_LALT)SS_DOWN(X_BSPC), SS_UP(X_LALT)SS_UP(X_BSPC), SS_DOWN(X_LCTL)SS_DOWN(X_BSPC), SS_UP(X_LCTL)SS_UP(X_BSPC)))
